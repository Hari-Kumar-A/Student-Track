<div class="covering">

    <div class="dashboard">
        <div class="heading">Your Expenses</div>
        <button> <a href="/expenses/addexpense">+New Expense</a></button>
    </div>
    
    <div class="link"> <a href="/">Dashboard</a> / <a href="/expenses">Events</a>/ Your Expenses</div>

     <div class="dotcovering">
      <% const options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' }; %>
        
        
        <div class="course"> Expense : <%= expensedata.expense %> </div>
        <div class="course"> Category : <%= expensedata.category %> </div>
        <div class="course"> Amount : <%= expensedata.amount %> Rupees </div>
        <div class="duedate"> Date : <%=  (expensedata.date).toLocaleDateString(undefined, options) %></div>
    </div>
    
</div>

